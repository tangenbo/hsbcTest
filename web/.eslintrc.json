{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "sourceType": "module",
        "parser": "babel-eslint"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "vue/valid-v-model": 0,
        "vue/valid-v-for": 0,
        "vue/require-v-for-key": 0,
        "no-sparse-arrays": 0,
        "vue/no-parsing-error": [
            2,
            {
                "x-invalid-end-tag": false
            }
        ],
        "no-console": 0
    }
}